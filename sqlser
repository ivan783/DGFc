	[ExchangeSale] [decimal](19, 6) NOT NULL,
	[Description] [nvarchar](80) NULL,
	[IsScheduledProcess] [bit] NOT NULL,
	[ScheduledProcess] [datetime] NOT NULL,
	[IsMultipleDebits] [bit] NOT NULL,
	[OperationNumberDebitHost] [nvarchar](8) NULL,
	[IsTicket] [bit] NOT NULL,
	[NumberTicket] [int] NOT NULL,
	[PreferentialExchange] [decimal](19, 6) NOT NULL,
	[IndicatorBuyOrSale] [nchar](1) NULL,
	[OperationStatusId] [int] NOT NULL,
	[ProcessBatchId] [int] NULL,
	[UserCreation] [int] NOT NULL,
	[UserModification] [int] NULL,
	[DateCreation] [datetime] NOT NULL,
	[DateModification] [datetime] NULL,
	[IsCallback] [bit] NOT NULL,
