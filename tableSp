CREATE PROCEDURE [cw].[SpUpdateAuthorizedBatch]
(
    @ProcessBatchId INT,
	@OperationStatusId INT
)
AS
BEGIN
    SET NOCOUNT ON;
    UPDATE [cw].[AuthorizedBatches]
    SET OperationStatusId = @OperationStatusId
    WHERE ProcessBatchId = @ProcessBatchId;
	select @@ROWCOUNT
END
GO

